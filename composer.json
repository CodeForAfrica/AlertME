{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "url": "https://github.com/sanis/laravel4-goutte.git",
      "type": "git"
    }
  ],
  "require": {
    "laravel/framework": "5.0.*",
    "pda/pheanstalk": "~3.0",
    "illuminate/html": "~5.0",
    "graham-campbell/markdown": "~3.0",
    "doctrine/dbal": "~2.5",
    "emil/inliner": "dev-master",
    "elvedia/goutte": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "phpspec/phpspec": "~2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Greenalert\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
